{
  "id": "mcp-hub",
  "name": "MCP Hub",
  "description": "Built-in MCP preset provider configuration hub.",
  "kind": "mcp",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "version": {
        "oneOf": [
          { "type": "number", "enum": [1, 2, 3] },
          { "type": "string", "enum": ["1", "2", "3"] }
        ]
      },
      "providers": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "additionalProperties": true
        },
        "default": {}
      },
      "builtinProviders": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "additionalProperties": true
        },
        "default": {}
      },
      "marketProviders": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "additionalProperties": true
        },
        "default": {}
      },
      "marketConfig": {
        "type": "object",
        "additionalProperties": true,
        "default": {}
      }
    }
  }
}
