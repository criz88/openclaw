  "kind": "mcp",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "version": {
        "type": "number",
        "enum": [2]
      },
      "providers": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "additionalProperties": true
        },
        "default": {}
      },
      "builtinProviders": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "additionalProperties": true
        },
        "default": {}
      },
      "marketProviders": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "additionalProperties": true
        },
        "default": {}
      },
      "marketConfig": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "registryBaseUrl": {
            "type": "string"
          },
          "apiKeyRef": {
            "type": "string"
          },
          "lastSyncAt": {
            "type": "string"
          }
        }
      }
    }
  }
}
